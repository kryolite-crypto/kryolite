{
  "Logging": {
    "Loglevel": {
      "Default": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.*": "Warning",
        "Yarp.*": "Warning"
      },
      "FormatterName": "clean",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": false,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss ",
        "UseUtcTimestamp": false
      }
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://localhost:5100"
      }
    }
  },
  "PublicUrl": "https://localhost:5100",
  "Peers": [

  ]
}
