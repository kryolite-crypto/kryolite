{
  "Logging": {
    "Loglevel": {
      "Default": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.Hosting": "Warning",
        "Microsoft.AspNetCore": "Warning"
      },
      "FormatterName": "clean",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": false,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss ",
        "UseUtcTimestamp": false
      }
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5000"
      }
    }
  },
  "NodeIp": "*",
  "NodePort": 6000,
  "Peers": [
    "http://daemon:6000"
  ]
}
