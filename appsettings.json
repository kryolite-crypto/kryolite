{
    "Logging": {
        "Loglevel": {
            "Default": "Information"
        },
        "Console": {
            "LogLevel": {
              "Default": "Information",
              "Microsoft.Hosting.Lifetime": "Warning"
            },
            "FormatterName": "clean",
            "FormatterOptions": {
                "SingleLine": true,
                "IncludeScopes": false,
                "TimestampFormat": "yyyy-MM-dd HH:mm:ss ",
                "UseUtcTimestamp": false
            }
        }
    }
}